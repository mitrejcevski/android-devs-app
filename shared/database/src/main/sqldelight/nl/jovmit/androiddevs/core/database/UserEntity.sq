CREATE TABLE UserEntity (
    uid TEXT PRIMARY KEY NOT NULL,
    email TEXT NOT NULL,
    about TEXT NOT NULL,
    favorite INTEGER NOT NULL
);

fetchAll:
SELECT * FROM UserEntity;

addUser:
INSERT INTO UserEntity(uid, email, about, favorite)
VALUES (?, ?, ?, ?);

addFullUser:
INSERT INTO UserEntity(uid, email, about, favorite)
VALUES ?;

deleteUser:
DELETE FROM UserEntity WHERE uid IN :uid;